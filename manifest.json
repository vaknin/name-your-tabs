{
    "manifest_version": 2,
    "name": "tempname",
    "description": "tempdesc",
    "version": "1.00",
    "content_scripts": [
      {
        "matches": [
          "<all_urls>"
        ],
        "js": ["./scripts/content.js"]
      }
    ],
    "background": {
      "scripts": ["./scripts/background.js"],
      "persistent": false
    },
    "icons": {
      "16": "./images/icon.png",
      "48": "./images/icon.png",
      "128": "./images/icon.png"
    },
    "commands": {
      "toggle-edit-mode": {
        "suggested_key": {
          "default": "Ctrl+Shift+E",
          "mac": "Command+Shift+E"
        },
        "description": "Toggle Naming Mode"
      },
      "favicon-red": {
        "suggested_key": {
          "default": "Ctrl+Shift+1",
          "mac": "Command+Shift+1"
        },
        "description": "Set the favicon to red"
      },
      "favicon-yellow": {
        "suggested_key": {
          "default": "Ctrl+Shift+2",
          "mac": "Command+Shift+2"
        },
        "description": "Set the favicon to yellow"
      },
      "favicon-green": {
        "suggested_key": {
          "default": "Ctrl+Shift+3",
          "mac": "Command+Shift+3"
        },
        "description": "Set the favicon to green"
      }
    },
    "permissions": [
      "tabs", "storage"
    ]
  }